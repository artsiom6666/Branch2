<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Average_Number_of_days_between_orders__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Number_of_Orders__c > 1 ,(Last_order_date__c - First_Order_Date__c) / ( Number_of_Orders__c - 1), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Average Number of Days Between Orders</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
