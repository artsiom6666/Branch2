<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_ROI__c</fullName>
    <description>The calculated Return on Investment is a Total Orders Value/Total Costs assuming the Total Costs is the summary value of the related costs fields.</description>
    <externalId>false</externalId>
    <formula>IF(ActualCost > 0, (Total_Orders_Value__c - ActualCost) / ActualCost, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>(Total_Orders_Value__c - ActualCost) / ActualCost</inlineHelpText>
    <label>Total ROI</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Percent</type>
</CustomField>
