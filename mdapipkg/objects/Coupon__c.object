    <validationRules>
        <fullName>Auto_Apply_Check</fullName>
        <active>true</active>
        <errorConditionFormula>Auto_Apply__c = TRUE 
&amp;&amp; Coupon_Code__c != NULL</errorConditionFormula>
        <errorDisplayField>Coupon_Code__c</errorDisplayField>
        <errorMessage>&quot;Coupon Code&quot; must be empty for Auto-Apply coupon.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Amount_Off</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL( Discount_Type__c , &apos;Discount %&apos;)|| ISPICKVAL( Discount_Type__c , &apos;$ Off&apos;)) 
&amp;&amp; ( Discount_Amount__c = 0 || ISNULL(Discount_Amount__c) = TRUE)</errorConditionFormula>
        <errorDisplayField>Discount_Amount__c</errorDisplayField>
        <errorMessage>Amount Off can&apos;t be NULL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt;= Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date must be more than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Uses_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>( ISPICKVAL( Uses_Limit__c , &apos;Overall&apos; ) ||  ISPICKVAL( Uses_Limit__c , &apos;Per Customer&apos; ) ) 
&amp;&amp; (  Uses_Limit_Value__c = 0 || ISNULL( Uses_Limit_Value__c ) = TRUE )</errorConditionFormula>
        <errorDisplayField>Uses_Limit_Value__c</errorDisplayField>
        <errorMessage>Uses Limit Value can&apos;t be NULL</errorMessage>
    </validationRules>
