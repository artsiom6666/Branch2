    <fields>
        <fullName>Total_CPA__c</fullName>
        <description>Cost Per Acquisition is a required formula field on the campaign that takes the Total Costs/Total Orders assuming the Total Costs is the summary value of the related costs fields</description>
        <externalId>false</externalId>
        <formula>IF(NumberOfContacts &gt; 0, ActualCost /  NumberOfContacts, null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>ActualCost /  NumberOfContacts</inlineHelpText>
        <label>Total CPA</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_CPL_Hierarchy__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(HierarchyNumberOfContacts &gt; 0, HierarchyNumberOfLeads &gt; 0), HierarchyActualCost / (HierarchyNumberOfContacts + HierarchyNumberOfLeads), null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>HierarchyActualCost / (HierarchyNumberOfContacts + HierarchyNumberOfLeads)</inlineHelpText>
        <label>Total CPL in Hierarchy</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_CPL__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(NumberOfContacts &gt; 0, NumberOfLeads &gt; 0), ActualCost / (NumberOfContacts + NumberOfLeads), null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>ActualCost / (NumberOfContacts + NumberOfLeads</inlineHelpText>
